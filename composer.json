{
    "name": "erilshk/vinti4net",
    "description": "SDK PHP para integração de pagamento via Vinti4Net (SISP, Cabo Verde)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Erilshk\\Vinti4Net\\": "src/",
            "Erilshk\\Sisp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Eril Shackle",
            "email": "erilandocarvalho@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
